#include <iostream>
#include <climits>
int findmin(int A[],int size){
    int min= INT_MAX;
    for(int i=0;i<size;i++){
        if((A[i]>=0)&&(A[i]<min))
           min=A[i];
    }
           return min;
        
    }
    

int main(){
//find minimum positive element
    int A[]={5,7,-4,-2,3,6};
    int size = sizeof(A)/sizeof(int);
    std::cout<<findmin(A,size);
}
