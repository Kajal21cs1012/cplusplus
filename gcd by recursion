// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int gcd(int a,int b)
{
    if(a==0)return b;
    else
    return gcd(b%a,a);
    

}
int main(){
    int a,b;
    cout<<"enter number 1"<<endl;
    cin>>a;
    cout<<"enter number 2"<<endl;
    cin>>b;
    
    cout<<gcd(a,b)<<endl;
}
