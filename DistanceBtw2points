#include <iostream>
#include<math.h>
using namespace std;
class point {
    public: int x,y;
};
float dist(point a1, point a2){
    return sqrt(pow((a1.x-a2.x),2)+pow((a1.y-a2.y),2));
}
int main()
{
//distance between two points with the help of given coordinates
    point a1,a2;
    a1.x=10;a1.y=20;
    a2.x=15;a2.y=5;
    cout<<dist(a1,a2);
    return 0;
    
}
